<refentry xml:id="threaded.count" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Threaded::count</refname>
  <refpurpose>操作</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Threaded::count</methodname>
   <void />
  </methodsynopsis>
  <para>
   返回对象的属性数量
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
  
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>计算对象中的属性数量</title>
    <programlisting role="php">
<![CDATA[
<?php
$safe = new Threaded();

while (count($safe) < 10) {
    $safe[] = count($safe);
}

var_dump(count($safe));
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
int(10)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

</refentry>

